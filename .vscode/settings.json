{
    "module-templates.engine": "handlebars",
    "module-templates.templates": {
      "react-component-mui": {
        "displayName": "React component MUI",
        "defaultPath": "src/components",
        "folder": "{{pascal name}}",
        "questions": {
          "name": "Component name",
        },
        "files": [
          {
            "name": "{{pascal name}}.tsx",
            "content": [
              "import React from 'react'",
              "",
              "import { Box } from '@mui/material'",
              "",
              "export const {{pascal name}} = (props): React.ReactElement => {",
              "  const {",
              "    ...otherProps",
              "  } = props",
              "",
              "  return (",
              "    <Box",
              "      {...otherProps}",
              "    >",
              "",
              "    </Box>",
              "  )",
              "}",
              "",
              "export default {{pascal name}}",
              ""
            ]
          },
          {
            "name": "index.tsx",
            "content": [
              "export * from './{{pascal name}}'",
              "export { default } from './{{pascal name}}'",
              ""
            ]
          }
        ]
      }
    }
  }